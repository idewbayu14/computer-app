generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
  relationMode = "prisma"
}

model Computer {
  id_produk String   @id @default(uuid())
  nama_produk String
  kategori String
  harga Int
  stok Int
  tanggal_ditambahkan DateTime @default(now())
  gambar_produk String
}
